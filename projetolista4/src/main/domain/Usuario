package domain;

public class Usuario {
    private String nome;
    private Long id;
    private String login;
    private String senha;


    public Usuario(String nome, Long id, String login, String senha) {
        this.nome = nome;
        this.id = id;
        this.login = login;
        this.senha = senha;
    }

    public String getLogin() {
        return login;
    }

    public void setLogin(String login) {
        this.login = login;
    }

    public String getSenha() {
        return senha;
    }

    public void setSenha(String senha) {
        this.senha = senha;
    }
}